{
    "manifest_version": 3,
    "name": "Privacy Settings Analyzer",
    "version": "1.0",
    "description": "Analyzes browser privacy settings, provides recommendations, and generates reports with CVSS severity levels",
    "permissions": [
      "privacy",
      "cookies",
      "storage",
      "tabs",
      "webNavigation",
      "webRequest"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }